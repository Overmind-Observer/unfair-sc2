bool libCB22438A_gt_BerserkersBloodModStart_Func (bool testConds, bool runActions) {
    // Variable Declarations
    playergroup lv_players;
    text lv_defaultDescription;

    // Automatic Variable Declarations
    playergroup auto5D8198C1_g;
    int auto5D8198C1_var;

    // Variable Initialization
    lv_players = PlayerGroupEmpty();
    lv_defaultDescription = StringExternal("Param/Value/lib_CB22438A_021CA37A");

    // Actions
    if (!runActions) {
        return true;
    }

    if ((GameAttributeGameValue("41") == "0001")) {
        auto5D8198C1_g = PlayerGroupAll();
        auto5D8198C1_var = -1;
        while (true) {
            auto5D8198C1_var = PlayerGroupNextPlayer(auto5D8198C1_g, auto5D8198C1_var);
            if (auto5D8198C1_var < 0) { break; }
            if ((GameAttributePlayerValue("42", auto5D8198C1_var) == "0001")) {
                PlayerGroupAdd(lv_players, auto5D8198C1_var);
            }

        }
        if ((PlayerGroupCount(lv_players) > 0)) {
            TextExpressionSetToken("Param/Expression/lib_CB22438A_310396C1", "A", libDBC4E2D2_gf_GetPlayerNamesColored(lv_players));
            HelpPanelAddTip(PlayerGroupAll(), libNtve_gf_FormatTipTitle(StringExternal("Param/Value/lib_CB22438A_298195BA"), libNtve_ge_TipType_NormalTip), (lv_defaultDescription + TextExpressionAssemble("Param/Expression/lib_CB22438A_310396C1")), StringExternal("Param/Value/lib_CB22438A_1009F291"), "Assets\\Textures\\btn-ability-zerg-dehaka-tyrannozor-barrageofspikes.dds");
            TriggerExecute(libCB22438A_gt_AddBuffForInitialUnits, true, false);
        }
        else {
            TriggerEnable(libCB22438A_gt_AddBuffForCreatedUnit, false);
        }
    }
    else {
        TriggerEnable(libCB22438A_gt_AddBuffForCreatedUnit, false);
    }
    return true;
}

