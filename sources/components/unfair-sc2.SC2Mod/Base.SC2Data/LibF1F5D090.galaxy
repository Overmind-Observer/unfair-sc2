//--------------------------------------------------------------------------------------------------
bool libF1F5D090_gt_AutoDefeatModStart_Func (bool testConds, bool runActions) {
    // Variable Declarations
    playergroup lv_players;

    // Automatic Variable Declarations
    playergroup auto8257F67D_g;
    int auto8257F67D_var;

    // Variable Initialization
    lv_players = PlayerGroupEmpty();

    // Actions
    if (!runActions) {
        return true;
    }

    if ((GameAttributeGameValue("130") == "0001")) {
        Wait(1.0, c_timeGame);
        auto8257F67D_g = PlayerGroupActive();
        auto8257F67D_var = -1;
        while (true) {
            auto8257F67D_var = PlayerGroupNextPlayer(auto8257F67D_g, auto8257F67D_var);
            if (auto8257F67D_var < 0) { break; }
            if ((GameAttributePlayerValue("131", auto8257F67D_var) == "0001")) {
                PlayerGroupAdd(lv_players, auto8257F67D_var);
            }

        }
        if ((PlayerGroupCount(lv_players) > 0)) {
            KickFromGame(lv_players);
            TextExpressionSetToken("Param/Expression/lib_F1F5D090_D1F6C5DF", "Players", libDBC4E2D2_gf_GetPlayerNamesColored(lv_players));
            HelpPanelAddTip(PlayerGroupAll(), libNtve_gf_FormatTipTitle(StringExternal("Param/Value/lib_F1F5D090_FF98A79D"), libNtve_ge_TipType_NormalTip), TextExpressionAssemble("Param/Expression/lib_F1F5D090_D1F6C5DF"), StringExternal("Param/Value/lib_F1F5D090_CBB02A48"), "Assets\\Textures\\decaltag_clanlogo_diff_playerproxy_00.dds");
        }

    }

    return true;
}

